class Solution:
    def minDeletionSize(self, strs: list[str]) -> int:
        n = len(strs[0])
        delete = 0
        for i in range(n):
            for j in range(len(strs)-1):
                if strs[j][i] > strs[j+1][i]:
                    delete +=1
                    break
        return delete
